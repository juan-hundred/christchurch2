          "_customHTML": "<section data-bs-version=\"5.1\" class=\"form01 stockm5\" group=\"Form\" mbr-class=\"{\n    'mbr-fullscreen': fullScreen,\n    'mbr-parallax-background': bg.parallax}\" plugins=\"formstyler, datepicker\">\n    <mbr-parameters>\n        <header>Size</header>\n        <input type=\"checkbox\" title=\"Full Screen\" name=\"fullScreen\">\n        <input type=\"checkbox\" title=\"Full Width\" name=\"fullWidth\" checked>\n        <input type=\"range\" inline title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"12\" step=\"1\" value=\"2\" condition=\"fullScreen == false\">\n        <input type=\"range\" inline title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"12\" step=\"1\" value=\"2\" condition=\"fullScreen == false\">\n        <input type=\"checkbox\" title=\"Reverse\" name=\"reverseContent\">\n        <header>Show/Hide</header>\n        <input type=\"checkbox\" title=\"Title\" name=\"showTitle\" checked>\n        <input type=\"color\" title=\"Secondary Color\" value=\"#960719\" name=\"titleColor\" condition=\"showTitle\" selected>\n        <input type=\"checkbox\" title=\"Description\" name=\"showDesc\">\n        <input type=\"color\" title=\"Description BG\" value=\"#ffeec7\" name=\"desc\" condition=\"showDesc\" selected>\n        <input type=\"checkbox\" title=\"Contacts\" name=\"showContacts\">\n        <input type=\"color\" title=\"Active Color\" value=\"#960719\" name=\"active\" condition=\"showContacts\" selected>\n        <input type=\"checkbox\" title=\"Social\" name=\"showSocial\">\n        <input type=\"range\" inline title=\"Social Amount\" name=\"soc\" min=\"1\" max=\"3\" step=\"1\" value=\"3\" condition=\"showSocial\">\n        <input type=\"color\" title=\"Social Color\" value=\"#ffffff\" name=\"socColor\" condition=\"showSocial\" selected>\n        <input type=\"color\" title=\"Social BG\" value=\"#960719\" name=\"socBg\" condition=\"showSocial\" selected>\n        <input type=\"checkbox\" title=\"Social Description\" name=\"showSocDesc\" condition=\"showSocial\" checked>\n        <input type=\"color\" title=\"Card BG\" value=\"#ffeec7\" name=\"card\" condition=\"showContacts || showSocial\" selected>\n        <input type=\"color\" title=\"Container BG\" value=\"#960719\" name=\"container\" selected>\n        <header>Form</header>\n        <input type=\"color\" title=\"Input BG\" value=\"#f5ebd3\" name=\"inputBg\" selected>\n        <input type=\"color\" title=\"Input Color\" value=\"#404349\" name=\"inputColor\" selected>\n        <header>Map</header>\n        <input type=\"map\" title=\"Map\" name=\"googleMap\" place-id=\"561 springfelid ave summmit nj\n\" value=\"561 springfelid ave summmit nj\n\">\n        <header>Background</header>\n        <fieldset type=\"background\" name=\"bg\" parallax>\n            <input type=\"image\" title=\"Image\" value=\"@ADDONS_PATH@/stockm5/components/_images/background1.jpg\">\n            <input type=\"color\" title=\"Color\" value=\"#960719\" selected>\n        </fieldset>\n        <header condition=\"bg.type === 'video'\">Fallback Image</header>\n        <input type=\"image\" title=\"Fallback Image\" value=\"@ADDONS_PATH@/stockm5/components/_images/background1.jpg\" name=\"fallBackImage\" condition=\"bg.type === 'video'\">\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\">\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#ffffff\" condition=\"overlay && bg.type !== 'color'\">\n        <input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.8\" condition=\"overlay && bg.type !== 'color'\">\n    <input type=\"hidden\" name=\"usedRecaptcha\" value=\"false\"></mbr-parameters>\n\n    <div class=\"mbr-fallback-image disabled\" mbr-if=\"bg.type == 'video'\"></div>\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type !== 'color'\" opacity=\"{{overlayOpacity}}\" bg-color=\"{{overlayColor}}\"></div>\n\n    <div mbr-class=\"{'container': !fullWidth, 'container-fluid': fullWidth}\">\n        <div class=\"row\">\n            <div class=\"col-12\">\n                <div class=\"content-wrap\" mbr-class=\"{'flex-row-reverse' : reverseContent}\">\n                    <div class=\"cont-wrap\">\n                        <div class=\"title-wrapper\" mbr-if=\"showDesc || showTitle\">\n                            <div class=\"desc-wrapper\">\n                                <p class=\"mbr-desc mbr-fonts-style\" mbr-theme-style=\"display-4\" mbr-if=\"showDesc\" data-app-selector=\".mbr-desc, .desc-wrapper\">\n                                    Contacts\n                                </p>\n                            </div>\n                            <h2 class=\"mbr-section-title mbr-fonts-style\" data-app-selector=\".mbr-section-title, .title-wrap\" mbr-theme-style=\"display-5\" mbr-if=\"showTitle\">\n                                <b>Contact Christ Church Summit&nbsp;<span><b>cooperation</b></span></b>\n                            </h2>\n                        </div>\n                        <div mbr-form class=\"mbr-form form-wrapper\">\n<!--Formbuilder Form-->\n<form action=\"juangonzalez633@gmail.com\" name=\"Form Name\" method=\"POST\" class=\"mbr-form form-with-styler\" data-verified=\"juangonzalez633@gmail.com\">\n<div class=\"row\">\n<div hidden=\"hidden\" data-form-alert class=\"alert alert-success col-12\">Thanks\nfor\nfilling out the form!</div>\n<div hidden=\"hidden\" data-form-alert-danger class=\"alert alert-danger col-12\"> Oops...! some problem! </div>\n</div>\n<div class=\"dragArea row\">\n<div class=\"col-lg-12 col-md-12 col-sm-12\">\n<p mbr-theme-style=\"display-7\" class=\"mbr-text mbr-fonts-style\"> Fill out the form and our staff will contact you </p>\n</div>\n<div class=\"col-lg-12 col-md-12 col-sm-12 form-group mb-3 mb-3\" data-for=\"name\" style>\n<input type=\"text\" name=\"name\" placeholder=\"Name\" data-form-field=\"name\" mbr-theme-style=\"display-4\" class=\"form-control\" required=\"required\" value>\n</div>\n<div data-for=\"email\" class=\"col-lg-12 col-md-12 col-sm-12 form-group mb-3 mb-3 mb-3 mb-3\">\n<input type=\"email\" name=\"email\" placeholder=\"Email\" data-form-field=\"email\" mbr-theme-style=\"display-4\" class=\"form-control\" required=\"required\" value>\n</div>\n<div data-for=\"number\" class=\"col-lg-12 col-md-12 col-sm-12 form-group\">\n<input type=\"number\" name=\"number\" placeholder=\"Number (Optional)\" data-form-field=\"number\" mbr-theme-style=\"display-7\" class=\"form-control\" max=\"100\" min=\"0\" step=\"1\" value>\n</div>\n<div class=\"col-lg-12 col-md-12 col-sm-12 form-group mb-3\" data-for=\"text\" style>\n<input type=\"text\" name=\"text\" placeholder=\"Message\" data-form-field=\"text\" mbr-theme-style=\"display-7\" class=\"form-control\" required=\"required\" value>\n</div>\n<div class=\"col-lg-12 col-md-12 col-sm-12 form-group\" data-for=\"date\" hidden=\"hidden\">\n<input type=\"date\" name=\"date\" data-form-field=\"date\" mbr-theme-style=\"display-7\" class=\"form-control\" value=\"2018-07-22\" hidden=\"hidden\">\n</div>\n<div class=\"col-lg-12 col-md-12 col-sm-12 form-group\" data-for=\"time\" hidden=\"hidden\">\n<input type=\"time\" name=\"time\" max=\"86400\" min=\"0\" step=\"1800\" data-form-field=\"time\" mbr-theme-style=\"display-7\" class=\"form-control\" value=\"10:00\" hidden=\"hidden\">\n</div>\n<div mbr-buttons=\"true\" mbr-theme-style=\"display-4\" data-toolbar=\"-mbrBtnMove,-mbrLink,-mbrBtnRemove,-mbrBtnAdd\" class=\"col mbr-section-btn\"><a type=\"submit\" class=\"w-100 btn btn-warning\" data-app-placeholder=\"Type Text\"> Send now </a></div>\n</div>\n</form><!--Formbuilder Form-->\n</div>\n                    </div>\n                    <div class=\"map-wrapper card\">\n                        <div class=\"google-map\" mbr-map=\"googleMap\"></div>\n                        <div class=\"contacts-wrapper\" mbr-if=\"showContacts || showSocial\">\n                            <div class=\"contacts-wrap\" mbr-if=\"showContacts\">\n                                <ul class=\"list mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".list, .item-wrap\" data-multiline mbr-article>\n                                    <li class=\"item-wrap\"><b>+98 765 43 21</b></li>\n                                    <li class=\"item-wrap\"><b>mobi@rise.com</b></li>\n                                    <li class=\"item-wrap\">La Paix 12, Paris</li>\n                                </ul>\n                            </div>\n                            <div class=\"social-wrapper\" mbr-if=\"showSocial\">\n                                <p class=\"soc-desc mbr-fonts-style\" mbr-theme-style=\"display-4\" mbr-if=\"showSocDesc\" data-app-selector=\".soc-desc, .social-wrapper\"><b>\n                                    Follow Us</b></p>\n                                <div class=\"social-wrap\">\n                                    <div class=\"soc-item\">\n                                        <a href=\"https://mobiri.se/\">\n                                            <span mbr-icon class=\"mbr-iconfont socicon socicon-facebook\"></span>\n                                        </a>\n                                    </div>\n                                    <div class=\"soc-item\" mbr-if=\"soc > 1\">\n                                        <a href=\"https://mobiri.se/\">\n                                            <span mbr-icon class=\"mbr-iconfont socicon socicon-instagram\"></span>\n                                        </a>\n                                    </div>\n                                    <div class=\"soc-item\" mbr-if=\"soc > 2\">\n                                        <a href=\"https://mobiri.se/\">\n                                            <span mbr-icon class=\"mbr-iconfont socicon socicon-tiktok\"></span>\n                                        </a>\n                                    </div>\n                                </div>\n                            </div>\n                        </div>\n                    </div>\n                </div>\n            </div>\n        </div>\n    </div>\n</section>",
          "_styles": {
            "& when not (@fullScreen)": {
              "padding-top": "(@paddingTop * 1rem)",
              "padding-bottom": "(@paddingBottom * 1rem)"
            },
            "& when (@bg-type = 'color')": {
              "background-color": "@bg-value"
            },
            "& when (@bg-type = 'image')": {
              "background-image": "url(@bg-value)"
            },
            ".mbr-fallback-image.disabled": {
              "display": "none"
            },
            ".mbr-fallback-image": {
              "display": "block",
              "background-size": "cover",
              "background-position": "center center",
              "width": "100%",
              "height": "100%",
              "position": "absolute",
              "top": "0",
              "& when (@bg-type = 'video')": {
                "background-image": "url(@fallBackImage)"
              }
            },
            ".content-wrap": {
              "padding": "60px",
              "background-color": "@container",
              "display": "flex",
              "@media (max-width: 992px)": {
                "padding": "30px 20px",
                "display": "block"
              },
              ".cont-wrap": {
                "width": "50%",
                "padding-right": "60px",
                "@media (max-width: 992px)": {
                  "width": "100%",
                  "padding-right": "0"
                },
                "& when (@reverseContent)": {
                  "padding-right": "0",
                  "padding-left": "60px",
                  "@media (max-width: 992px)": {
                    "padding-left": "0"
                  }
                },
                ".title-wrapper": {
                  "margin-bottom": "54px",
                  "@media (max-width: 992px)": {
                    "margin-bottom": "40px"
                  },
                  ".desc-wrapper": {
                    ".mbr-desc": {
                      "display": "inline-block",
                      "background-color": "@desc",
                      "padding": "8px 16px",
                      "margin-bottom": "16px"
                    }
                  },
                  ".mbr-section-title": {
                    "margin-bottom": "0",
                    "span": {
                      "color": "@titleColor"
                    }
                  }
                },
                ".form-wrapper": {
                  "padding-right": "130px",
                  "@media (max-width: 1200px)": {
                    "padding-right": "0",
                    "margin-bottom": "40px"
                  },
                  "& when (@reverseContent)": {
                    "padding-right": "0",
                    "padding-left": "60px",
                    "@media (max-width: 1200px)": {
                      "padding-left": "0"
                    }
                  },
                  ".dragArea.row": {
                    "position": "relative",
                    "z-index": "1",
                    ".mbr-text": {
                      "margin-bottom": "30px",
                      "@media (max-width: 992px)": {
                        "margin-bottom": "20px"
                      }
                    },
                    ".form-group": {
                      "margin-bottom": "20px !important",
                      ".form-control-label": {
                        "width": "100%",
                        "margin-bottom": "10px"
                      },
                      ".form-control": {
                        "position": "relative",
                        "z-index": "1",
                        "padding": "22px 20px",
                        "border": "none !important",
                        "box-shadow": "none",
                        "background-color": "@inputBg",
                        "line-height": "1 !important",
                        "@media (max-width: 992px)": {
                          "padding": "22px"
                        },
                        "&:hover, &:focus": {
                          "border": "none !important"
                        },
                        "&::placeholder": {
                          "color": "@inputColor",
                          "font-size": "18px",
                          "opacity": ".6"
                        }
                      },
                      "textarea": {
                        "min-height": "100px"
                      },
                      ".form-check": {
                        "padding-left": "2.3em",
                        "margin-bottom": "16px",
                        ".form-check-input": {
                          "background-color": "@inputColor",
                          "border-color": "@inputColor"
                        }
                      }
                    },
                    ".mbr-section-btn": {
                      "width": "100%",
                      ".btn": {
                        "width": "100%",
                        "margin-top": "0"
                      }
                    }
                  }
                }
              },
              ".map-wrapper": {
                "position": "relative",
                "display": "flex",
                "justify-content": "flex-end",
                "padding": "10px",
                "width": "50%",
                "@media (max-width: 992px)": {
                  "width": "100%",
                  "padding-top": "250px"
                },
                ".google-map": {
                  "position": "absolute",
                  "top": "0",
                  "left": "0",
                  "width": "100%",
                  "height": "100%",
                  "iframe": {
                    "position": "absolute",
                    "top": "0",
                    "left": "0",
                    "width": "100%",
                    "height": "100%"
                  }
                },
                ".contacts-wrapper": {
                  "position": "relative",
                  "z-index": "1",
                  "display": "flex",
                  "background-color": "@card",
                  "padding": "30px",
                  "@media (max-width: 1200px)": {
                    "display": "block"
                  },
                  "@media (max-width: 992px)": {
                    "padding": "20px 10px 10px"
                  },
                  ".contacts-wrap": {
                    "width": "55%",
                    "padding-right": "20px",
                    "@media (max-width: 1200px)": {
                      "padding-right": "0",
                      "width": "100%",
                      "margin-bottom": "20px"
                    },
                    "& when (@reverseContent)": {
                      "padding-right": "0",
                      "padding-left": "20px",
                      "@media (max-width: 1200px)": {
                        "padding-left": "0"
                      }
                    },
                    ".list": {
                      "padding": "0",
                      "margin": "0",
                      "list-style-type": "none",
                      ".item-wrap": {
                        "margin-bottom": "0",
                        "cursor": "pointer",
                        "transition": "all 0.3s ease-in-out",
                        "&:hover, &:focus": {
                          "color": "@active"
                        }
                      }
                    }
                  },
                  ".social-wrapper": {
                    "width": "45%",
                    "@media (max-width: 1200px)": {
                      "width": "100%"
                    },
                    ".soc-desc": {
                      "margin-bottom": "16px",
                      "opacity": ".6"
                    },
                    ".social-wrap": {
                      "display": "inline-flex",
                      "flex-wrap": "wrap",
                      ".soc-item": {
                        "a": {
                          "display": "block",
                          "&:hover, &:hover": {
                            ".mbr-iconfont": {
                              "transform": "scale(1.1)"
                            }
                          },
                          ".mbr-iconfont": {
                            "display": "flex",
                            "align-items": "center",
                            "justify-content": "center",
                            "width": "40px",
                            "height": "40px",
                            "min-width": "40px",
                            "color": "@socColor",
                            "background-color": "@socBg",
                            "font-size": "16px",
                            "border-radius": "100%",
                            "transition": "all 0.3s ease-in-out",
                            "margin": "0 10px 10px 0"
                          }
                        }
                      }
                    }
                  }
                }
              }
            },
            ".mbr-section-title": {
              "color": "#404349"
            },
            ".mbr-desc": {
              "color": "#b19a7c"
            },
            ".mbr-text": {
              "color": "#ffeec7"
            },
            ".list": {
              "color": "#404349"
            },
            ".soc-desc": {
              "color": "#404349"
            },
            "label": {
              "color": "#404349"
            },
            ".soc-desc, .social-wrapper": {
              "color": "#960719"
            },
            ".mbr-section-title, .title-wrap": {
              "color": "#ffeec7"
            },
            ".mbr-desc, .desc-wrapper": {
              "color": "#960719"
            }
          },
