          "_name": "footer01",
          "_sourceTheme": "essencem5",
          "_customHTML": "<section data-bs-version=\"5.1\" class=\"footer01 essencem5\" group=\"Footer\" global once=\"footers\" mbr-class=\"{\n    'mbr-fullscreen': fullScreen,\n    'mbr-parallax-background': bg.parallax}\">\n    <mbr-parameters>\n        <header>Size</header>\n        <input type=\"checkbox\" title=\"Full Screen\" name=\"fullScreen\">\n        <input type=\"checkbox\" title=\"Full Width\" name=\"fullWidth\" checked>\n        <input type=\"range\" inline title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"12\" step=\"1\" value=\"0\" condition=\"fullScreen == false\">\n        <input type=\"range\" inline title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"12\" step=\"1\" value=\"0\" condition=\"fullScreen == false\">\n        <input type=\"checkbox\" title=\"Reverse\" name=\"reverseContent\" checked>\n        <header>Show/Hide</header>\n        <input type=\"checkbox\" title=\"Title\" name=\"showTitle\" checked>\n        <input type=\"checkbox\" title=\"Subtitle\" name=\"showSubtitle\" checked>\n        <input type=\"checkbox\" title=\"Text\" name=\"showText\" checked>\n        <input type=\"checkbox\" title=\"Navigation\" name=\"showNav\" checked>\n        <input type=\"checkbox\" title=\"Contacts\" name=\"showContacts\" checked>\n        <input type=\"range\" inline title=\"Amount\" name=\"cont\" min=\"0\" max=\"3\" step=\"1\" value=\"3\" condition=\"showContacts\">\n        <input type=\"checkbox\" title=\"Icon\" name=\"showIcon\" condition=\"showContacts\" checked>\n        <input type=\"color\" title=\"Icon Color\" value=\"#f5ebd3\" name=\"icon\" condition=\"showContacts\" selected>\n        <input type=\"checkbox\" title=\"Desc\" name=\"showDesc\" checked>\n        <input type=\"checkbox\" title=\"Buttons\" name=\"showButtons\">\n        <input type=\"checkbox\" title=\"Social\" name=\"showSocial\" checked>\n        <input type=\"range\" inline title=\"Social Amount\" name=\"soc\" min=\"1\" max=\"4\" step=\"1\" value=\"4\" condition=\"showSocial\">\n        <input type=\"color\" title=\"Social Color\" value=\"#ffeec7\" name=\"socColor\" condition=\"showSocial\" selected>\n        <input type=\"color\" title=\"Active Color\" value=\"#e6c16d\" name=\"active\" condition=\"showNav || showContacts || showSocial\" selected>\n        <input type=\"checkbox\" title=\"Border\" name=\"showBorder\" checked>\n        <input type=\"color\" title=\"Border Color\" value=\"#b0b0b0\" name=\"border\" condition=\"showBorder\" selected>\n        <input type=\"color\" title=\"Shadow Color\" value=\"#960719\" name=\"shadow\" selected>\n        <input type=\"checkbox\" title=\"Copy\" name=\"showCopy\" checked>\n        <header>Background</header>\n        <fieldset type=\"background\" name=\"bg\" parallax>\n            <input type=\"image\" title=\"Image\" value=\"@ADDONS_PATH@/essencem5/components/_images/background12.jpg\" parallax>\n            <input type=\"color\" title=\"Color\" value=\"#960719\" selected>\n        </fieldset>\n        <header condition=\"bg.type === 'video'\">Fallback Image</header>\n        <input type=\"image\" title=\"Fallback Image\" value=\"@ADDONS_PATH@/essencem5/components/_images/background1.jpg\" name=\"fallBackImage\" condition=\"bg.type === 'video'\">\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\" checked>\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#000000\" condition=\"overlay && bg.type !== 'color'\">\n        <input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.4\" condition=\"overlay && bg.type !== 'color'\">\n    </mbr-parameters>\n\n    <div class=\"mbr-fallback-image disabled\" mbr-if=\"bg.type == 'video'\"></div>\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type !== 'color'\" opacity=\"{{overlayOpacity}}\" bg-color=\"{{overlayColor}}\"></div>\n\n    <div mbr-class=\"{'container': !fullWidth, 'container-fluid': fullWidth}\">\n        <div class=\"row row-main\" mbr-class=\"{'flex-row-reverse' : reverseContent}\">\n            <div class=\"col-12 col-lg-3 card\" mbr-if=\"showTitle || showText\">\n                <div class=\"title-wrapper\">\n                    <h2 class=\"mbr-section-title mbr-fonts-style\" data-app-selector=\".mbr-section-title\" mbr-theme-style=\"display-5\" mbr-if=\"showTitle\">\n                        Please Support Us</h2>\n                    <p class=\"mbr-text mbr-fonts-style\" data-app-selector=\".mbr-text\" mbr-theme-style=\"display-7\" mbr-if=\"showText\"><br><br><b>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp;<a href=\"https://ccsnj.breezechms.com/give/online\" class=\"text-primary\">DONATE</a></b><br><b><br></b><br><br><br></p>\n                </div>\n            </div>\n            <div class=\"col-12 col-lg-3 card\" mbr-if=\"showNav\">\n                <div class=\"nav-wrapper\">\n                    <h3 class=\"mbr-section-subtitle mbr-fonts-style\" data-app-selector=\".mbr-section-subtitle\" mbr-theme-style=\"display-5\" mbr-if=\"showSubtitle\">\n                        Main Links</h3>\n                    <ul class=\"list mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".list, .item-wrap\" data-multiline mbr-article>\n                        <li class=\"item-wrap\">&nbsp;&nbsp;<a href=\"about.html\" class=\"text-primary\">About</a></li>\n                        <li class=\"item-wrap\">&nbsp;&nbsp;<a href=\"events.html\" class=\"text-primary\">Events</a></li>\n                        <li class=\"item-wrap\">&nbsp;&nbsp;<a href=\"programs.html\" class=\"text-primary\">Programs</a></li><li class=\"item-wrap\">&nbsp; <a href=\"social.html\" class=\"text-primary\">Photos</a></li>\n                        <li class=\"item-wrap\"><span style=\"background-color: transparent; font-size: 1.25rem; color: rgb(255, 238, 199);\">&nbsp;&nbsp;</span><a href=\"members.html\" class=\"text-primary\" style=\"background-color: transparent; font-size: 1.25rem;\">Members</a><span style=\"background-color: transparent; font-size: 1.25rem; color: rgb(255, 238, 199);\">&nbsp;</span><br></li><li class=\"item-wrap\"><span style=\"font-size: 1.25rem; background-color: transparent;\">&nbsp;&nbsp;<a href=\"https://www.cornerstonekids.org/\" class=\"text-primary\">Cornerstone School&nbsp;</a></span><br></li>\n                    </ul>\n                </div>\n            </div>\n            <div class=\"col-12 col-lg-3 card\" mbr-if=\"showContacts\">\n                <div class=\"contacts-wrapper\">\n                    <h3 class=\"mbr-section-subtitle mbr-fonts-style\" data-app-selector=\".mbr-section-subtitle\" mbr-theme-style=\"display-5\" mbr-if=\"showSubtitle\">\n                        Contact<br>\n                    </h3>\n                    <div class=\"contacts-wrap\">\n                        <div class=\"item-contact\">\n                            <div class=\"item-wrap\">\n                                <span mbr-icon class=\"mbr-iconfont mobi-mbri-map-pin mobi-mbri\" mbr-if=\"showIcon\"></span>\n                                <a href=\"#\">\n                                    <p class=\"mbr-contact mbr-fonts-style\" data-app-selector=\".mbr-contact, .item-contact\" mbr-theme-style=\"display-7\">&nbsp;<br>561 Springfield Avenue\n<br>Summit, New Jersey 0790<br></p>\n                                </a>\n                            </div>\n                        </div>\n                        <div class=\"item-contact\" mbr-if=\"cont > 1\">\n                            <div class=\"item-wrap\">\n                                <span mbr-icon class=\"mbr-iconfont mobi-mbri-phone mobi-mbri\" mbr-if=\"showIcon\"></span>\n                                <a href=\"#\">\n                                    <p class=\"mbr-contact mbr-fonts-style\" data-app-selector=\".mbr-contact, .item-contact\" mbr-theme-style=\"display-7\">\n                                        908-273-5549\n                                    </p>\n                                </a>\n                            </div>\n                        </div>\n                        <div class=\"item-contact\" mbr-if=\"cont > 2\">\n                            <div class=\"item-wrap\">\n                                <span mbr-icon class=\"mbr-iconfont mobi-mbri-letter mobi-mbri\" mbr-if=\"showIcon\"></span>\n                                <a href=\"#\">\n                                    <p class=\"mbr-contact mbr-fonts-style\" data-app-selector=\".mbr-contact, .item-contact\" mbr-theme-style=\"display-7\">\n                                        brenda@ccsnj.org<br>\n                                    </p>\n                                </a>\n                            </div>\n                        </div>\n                    </div>\n                </div>\n            </div>\n            <div class=\"col-12 col-lg-3 card\" mbr-if=\"showSubtitle || showDesc || showButtons || showSocial\">\n                <div class=\"desc-wrapper\">\n                    <h3 class=\"mbr-section-subtitle mbr-fonts-style\" data-app-selector=\".mbr-section-subtitle\" mbr-theme-style=\"display-5\" mbr-if=\"showSubtitle\">\n                        News\n                    </h3>\n                    <p class=\"mbr-desc mbr-fonts-style\" data-app-selector=\".mbr-desc, .mbr-section-btn\" mbr-theme-style=\"display-7\" mbr-if=\"showDesc\">\n                        Follow Us to keep up to date <br>with the latest news.<br><br><br><br>\n                    </p>\n                    <div mbr-buttons mbr-theme-style=\"display-4\" class=\"mbr-section-btn\" mbr-if=\"showButtons\" data-toolbar=\"-mbrBtnMove\"><a class=\"btn btn-primary-outline\" href=\"https://mobiri.se\" data-app-placeholder=\"Type Text\">\n                            Subscribe\n                        </a></div>\n                    <div class=\"social-wrapper\" mbr-if=\"showSocial\">\n                        <div class=\"social-wrap\">\n                            <div class=\"soc-item\">\n                                <a href=\"https://www.facebook.com/Christ.Church.Summit?sk=wall\" target=\"_blank\">\n                                    <span mbr-icon class=\"mbr-iconfont socicon-facebook socicon\"></span>\n                                </a>\n                            </div>\n                            <div class=\"soc-item\" mbr-if=\"soc > 1\">\n                                <a href=\"https://www.youtube.com/channel/UCFAoOToV5tIcyXGZK3VITTg\" target=\"_blank\">\n                                    <span mbr-icon class=\"mbr-iconfont socicon-youtube socicon\"></span>\n                                </a>\n                            </div>\n                            <div class=\"soc-item\" mbr-if=\"soc > 2\">\n                                <a href=\"https://www.ucc.org/what-we-do/justice-local-church-ministries/justice/health-and-wholeness-advocacy-ministries/lgbt/\" target=\"_blank\">\n                                    <span mbr-icon class=\"mbr-iconfont imind-rainbow\"></span>\n                                </a>\n                            </div>\n                            <div class=\"soc-item\" mbr-if=\"soc > 3\">\n                                <a href=\"https://www.instagram.com/christchurchsummit/\" target=\"_blank\">\n                                    <span mbr-icon class=\"mbr-iconfont socicon-instagram socicon\"></span>\n                                </a>\n                            </div>\n                        </div>\n                    </div>\n                </div>\n            </div>\n        </div>\n        <div class=\"row\">\n            <div class=\"col-12\">\n                <p class=\"mbr-copy mbr-fonts-style\" data-app-selector=\".mbr-copy\" mbr-theme-style=\"display-7\" mbr-if=\"showCopy\">\n                    © Copyright 2024 Christ Church Summit - All Rights Reserved\n                </p>\n            </div>\n      